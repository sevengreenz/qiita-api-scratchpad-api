{
  "name": "qiita-api-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "style:check": "tslint -c tslint.json --type-check --project client/tsconfig.json 'client/**/*.ts' && tslint -c tslint.json --type-check --project server/tsconfig.json 'server/**/*.ts'",
    "style:fix": "tslint -c tslint.json --fix --type-check --project client/tsconfig.json 'client/**/*.ts' && tslint -c tslint.json --fix --type-check --project server/tsconfig.json 'server/**/*.ts'",
    "vuetype": "vuetype client/components client/pages",
    "build:server": "tsc -p server",
    "build:client": "webpack --config client/webpack.config.js --context client && cp client/index.html build/client/index.html",
    "dev:server": "tsc -p server && sls offline",
    "dev:client": "webpack-dev-server --inline --config client/webpack.config.js --context client",
    "test:server": "mocha --compilers ts:espower-typescript/guess 'server/tests/**/*.ts'",
    "test:client": "mocha --compilers ts:espower-typescript/guess 'client/tests/**/*.ts'",
    "deploy": "NODE_ENV=producion npm run build:client && npm run build:server && serverless deploy && serverless s3deploy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^8.0.47",
    "@types/query-string": "^5.0.1",
    "aws-lambda": "^0.1.2",
    "axios": "^0.16.2",
    "core-js": "^2.5.1",
    "query-string": "^5.0.1",
    "vue": "^2.5.2",
    "vue-class-component": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "^0.15.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.16",
    "@types/mocha": "^2.2.44",
    "aws-lambda": "^0.1.2",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "espower-typescript": "^8.1.1",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4",
    "serverless": "^1.23.0",
    "serverless-offline": "^3.16.0",
    "serverless-s3-deploy": "^0.5.0",
    "ts-loader": "^2.3.7",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.5.3",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "vuetype": "^0.2.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
