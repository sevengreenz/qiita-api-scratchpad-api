  "description": "",
  "main": "index.js",
  "scripts": {
    "style:check":
      "tslint -e './node_modules/**/*.ts' --project tsconfig.json './**/*.ts'",
    "style:fix":
      "tslint -e './node_modules/**/*.ts' --fix --project tsconfig.json './**/*.ts'",
