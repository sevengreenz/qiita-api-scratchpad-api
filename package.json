    "deploy": "npm run build && SLS_DEBUG=* serverless deploy --stage prod -v"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^8.0.47",
    "@types/query-string": "^5.0.1",
    "aws-lambda": "^0.1.2",
    "axios": "^0.18.0",
    "core-js": "^2.5.1",
    "query-string": "^5.0.1",
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.16",
    "@types/jest": "^22.1.0",
    "@types/source-map-support": "^0.4.1",
    "dotenv-webpack": "^1.5.4",
    "espower-typescript": "^8.1.1",
    "jest": "^22.1.4",
    "serverless": "^1.27.3",
    "serverless-domain-manager": "^2.3.6",
    "serverless-offline": "^3.25.4",
    "serverless-prune-plugin": "^1.3.0",
    "serverless-s3-deploy": "^0.5.0",
    "ts-jest": "^22.0.1",
    "ts-loader": "^2.3.7",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
